{
    "name": "vscode-swagger2ts-plugin",
    "displayName": "vscode-swagger2ts-plugin",
    "description": "这是一个swagger接口出参转ts的插件",
    "version": "0.0.14",
    "license": "IMT",
    "publisher": "gaofengye",
    "repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-sample",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-swagger2ts-plugin.swagger2ts"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-swagger2ts-plugin.swagger2ts",
                "title": "swagger2ts"
            }
        ],
        "configuration": {
            "title": "swagger-ts-plugin",
            "properties": {
                "swagger-ts-plugin.serviceList": {
                    "type": "array",
                    "default": [
                        "sms-service"
                    ],
                    "description": "服务列表"
                },
                "swagger-ts-plugin.appUrl": {
                    "type": "string",
                    "default": "http://eureka.dev.com:1111/eureka/apps",
                    "description": "必须提供 http://eureka.dev.com:1111(eureka地址)+ /eureka/apps"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    },
    "dependencies": {
        "swagger-ts-plugin": "^1.1.17"
    }
}
